{
  "Covid19Api": {
    "UrlBase": "https://api.covid19api.com/",
    "Summary": "summary",
    "Countries": "countries",
    "DayOne": "dayone/country/{countryName}/status/confirmed",
    "DayOneLive": "dayone/country/{countryName}/status/confirmed/live",
    "DayOneTotal": "total/dayone/country/{countryName}/status/confirmed",
    "ByCountry": "country/{countryName}/status/{status}",
    "ByCountryLive": "country/{countryName}/status/{status}/live",
    "ByCountryTotal": "total/country/{countryName}/status/{status}",
    "LiveByCountryAndStatus": "live/country/{countryName}/status/{status}",
    "LiveByCountryAndStatusAfterDate": "live/country/{countryName}/status/{status}/date/{date}",
    "Stats": "stats"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
