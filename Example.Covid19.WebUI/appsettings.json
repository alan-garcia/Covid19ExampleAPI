{
  "AllowedHosts": "*",
  "Covid19Api": {
    "UrlBase": "https://api.covid19api.com/",
    "Summary": "summary",
    "Countries": "countries",
    "DayOne": "dayone/country/{countryName}/status/{status}",
    "DayOneLive": "dayone/country/{countryName}/status/{status}/live",
    "DayOneTotal": "total/dayone/country/{countryName}/status/{status}",
    "ByCountry": "country/{countryName}/status/{status}?from={dateFrom}&to={dateTo}",
    "ByCountryLive": "country/{countryName}/status/{status}/live",
    "ByCountryTotal": "total/country/{countryName}/status/{status}",
    "LiveByCountryAndStatus": "live/country/{countryName}/status/{status}",
    "LiveByCountryAndStatusAfterDate": "live/country/{countryName}/status/{status}/date/{date}",
    "Stats": "stats"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
